{
  "name": "epam-react-tutorial",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --env.development --config=config/webpack.config.dev.js --watch",
    "build": "npm run build:clean && webpack --env.production --config=config/webpack.config.prod.js",
    "build:clean": "rm -rf ./dist",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.1",
    "jest": "^22.4.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "redux": "^3.7.2"
  },
  "jest": {
    "testURL": "http://localhost",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.d.{js,jsx}"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(spec))\\.(js?|jsx?)$",
    "coverageThreshold": {
      "global": {
        "branches": 20,
        "functions": 20,
        "lines": 20,
        "statements": 20
      }
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "./src/setup-tests.js",
    "moduleDirectories": [
      "src",
      "node_modules"
    ]
  },
  "devDependencies": {
    "@types/autoprefixer": "^6.7.3",
    "@types/copy-webpack-plugin": "^4.4.2",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "@types/react-hot-loader": "^4.1.0",
    "@types/typescript": "^2.0.0",
    "autoprefixer": "^9.1.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-css-modules": "^3.4.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^5.0.3",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest-localstorage-mock": "^2.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.10",
    "react-hot-loader": "^4.3.11",
    "react-to-print": "^2.0.0-alpha-4",
    "sass-loader": "^6.0.6",
    "source-map": "^0.7.0",
    "style-loader": "^0.20.1",
    "ts-loader": "^5.2.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.1",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "^0.6.2",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  }
}
